{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    // "parserOptions": {
    //     "ecmaVersion": "latest",
    //     "sourceType": "module"
    // },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [
            "eror",
            4
        ],
        "linebreak-style": [
            "eror",
            "unix"
        ],
        "quotes": [
            "eror",
            "single"
        ],
        "semi": [
            "eror",
            "always"
        ]
    }
}
